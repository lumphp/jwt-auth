{
    "name": "lumphp/jwt-auth",
    "description": "JSON Web Token Authentication forked from tymondesigns/jwt-auth",
    "version": "0.1.0",
    "keywords": [
        "auth",
        "authentication",
        "json web token",
        "jwt",
        "lumphp"
    ],
    "homepage": "https://github.com/lumphp/jwt-auth",
    "support": {
        "issues": "https://github.com/lumphp/jwt-auth/issues",
        "source": "https://github.com/lumphp/jwt-auth"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Tymon",
            "email": "tymon148@gmail.com",
            "homepage": "https://tymon.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "lumphp/laravel-container": "^8.0",
        "lumphp/laravel-queue": "^8.0",
        "lumphp/laravel-auth": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-http": "^8.0",
        "lumphp/laravel-support": "^8.0",
        "lcobucci/jwt": "3.3.0",
        "namshi/jose": "^7.2",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Tymon\\JWTAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tymon\\JWTAuth\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "JWTAuth": "Tymon\\JWTAuth\\Facades\\JWTAuth",
                "JWTFactory": "Tymon\\JWTAuth\\Facades\\JWTFactory"
            },
            "providers": [
                "Tymon\\JWTAuth\\Providers\\LumenServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit --colors=always",
        "test:ci": "composer test -- --verbose --coverage-text --coverage-clover=coverage.xml"
    }
}
